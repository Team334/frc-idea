<idea-plugin version="2">
    <id>com.zelkatani.frc</id>
    <name>FRC Intellij Plugin</name>
    <version>1.0</version>
    <vendor email="zelkatani@gmail.com" url="https://github.com/zelkatani">Zakaria Elkatani</vendor>

    <description><![CDATA[
        WPILib Plugin for FRC teams who use IntelliJ
    ]]></description>

    <change-notes><![CDATA[
        In Development
    ]]></change-notes>

    <idea-version since-build="145.0"/>

    <extensions defaultExtensionNs="com.intellij">
        <!-- No extensions -->
    </extensions>

    <actions>
        <group text="WPILib">
            <add-to-group group-id="MainMenu" relative-to-action="RunMenu" anchor="after"/>
            <action class="com.team334.frcplugin.wizard.WPILibInstall" id="InstallWPILib"
                    text="Install WPILib" description="First time installation of WPILib"
            />
            <action class="com.team334.frcplugin.deploy.DeployAction" id="DeployAction"
                    text="Deploy" description="Deploy robot code"
            />
            <action class="com.team334.frcplugin.wizard.WPILibProperties" id="WPILibProperties"
                    text="WPILib Properties" description="WPILib Properties"
            />
        </group>
    </actions>

</idea-plugin>